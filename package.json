  "scripts": {
    "start": "/bin/sh run-dev.sh",
    "dev": "DEBUG=maphubs:* nodejs-dashboard -- node --max-old-space-size=4096 src/app.js",
    "load-dev-env": "/bin/sh ./load-dev-env.sh",
    "webpack": "NODE_ENV=production node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --config src/webpack.config.min.js --progress",
    "webpack-stats": "NODE_ENV=production node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --profile --json --config src/webpack.config.min.js > stats.json",
    "debug": "/bin/sh run-debug.sh",
    "prod-test": "/bin/sh run-prod-test.sh",
    "changelog": "github_changelog_generator && git add CHANGELOG.md && git commit -m \"update changelog\" && git push",
    "preversion": "",
    "version": "npm run webpack && git add -A assets && git add src/webpack-assets.json",
    "postversion": "git push && git push --tags && npm run sync-cdn && npm run changelog",
    "deploy-patch": "npm version patch --force -m \"version %s\"",
    "deploy-minor": "npm version minor --force -m \"version %s\"",
    "sync-cdn": "/bin/sh sync-to-cdn.sh",
    "sync-assets": "aws s3 sync assets/ s3://maphubs-assets --acl public-read --profile kris",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
